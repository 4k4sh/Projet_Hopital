<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.chart.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="background"  xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controleur.ReportingController">
    <children>
         
      <PieChart id="piechart" fx:id="piechart" layoutX="93.0" layoutY="114.0" prefHeight="300.0" prefWidth="500.0" styleClass="chart-pie-label" />
      <Label id="text" layoutX="100.0" layoutY="14.0" styleClass="label-header"  text="Choisissez votre graphique" />
      <ComboBox fx:id="comb" layoutX="250.0" layoutY="81.0" onAction="#combAction" prefWidth="150.0" />
      <BarChart fx:id="barchart" layoutX="93.0" layoutY="114.0" prefHeight="300.0" prefWidth="500.0" visible="false">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
         
    </children>
</AnchorPane>
